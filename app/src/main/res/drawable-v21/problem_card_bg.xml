<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
		android:color="?android:attr/colorControlHighlight">
	<item
		android:id="@android:id/mask"
		android:drawable="@color/problemHeaderPressed"/>
	<item android:state_pressed="true">
		<shape>
			<solid android:color="@color/problemHeaderPressed"/>
		</shape>
	</item>
	<item android:state_activated="true">
		<shape>
			<solid android:color="@color/problemHeaderActivated"/>
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="@color/problemHeaderColor"/>
		</shape>
	</item>
</ripple>